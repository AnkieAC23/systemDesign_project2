generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model outfits {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  photoURL  String
  date      DateTime @db.Date
  title     String?
  brand     String?
  occasion  String?
  rating    Int?
  notes     String?
}
